generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//!!!!!!!!  https://www.ozon.ru/category/smartfony-15502/

model telephone {
  id      Int     @id @default(autoincrement())
  model    String
  price Int
  date_of_sale DateTime
  processor String
  screen_diagonal Float 
  battery_capacity Int
  color    String?
  extra_options Int?
  wireless_interfaces String?
}



model user {
  id      Int     @id @default(autoincrement())
  email   String  @unique
  name    String?
  surname String?
  city    String?
}

 
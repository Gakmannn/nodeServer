generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model news {
  id    Int     @id @default(autoincrement())
  text  String 
  
}
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//!!!!!!!!  https://www.ozon.ru/category/smartfony-15502/

model telephone {
  id      Int     @id @default(autoincrement())
  model    String
  price Int
  date_of_sale String?
  screen_diagonal Int
  battery_capacity Int
  processor Int
  color    String?
  wireless_interfaces String?
}



model user {
  id      Int     @id @default(autoincrement())
  email   String  @unique
  name    String?
  surname String?
  city    String?
}

model news {
  id   Int    @id @default(autoincrement())
  text String
}

model Vehicles {
  id    Int    @id @default(autoincrement())
  model String
  color String
  year  Int
}

model Cities {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}
